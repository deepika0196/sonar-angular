{
  "name": "prototipo-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 8080 --host localhost.gva.es --disable-host-check",
    "build": "ng build --base-href /expropia-frontend/",
    "watch": "ng build --watch --configuration development",
    "zip-dist": "node zip.ts ./dist ./dist.zip",
    "serve": "ng serve --serve-path /expropia-frontend/",
    "prod": "ng serve --serve-path /expropia-frontend/ --prod",
    "pre": "ng serve --serve-path /expropia-frontend/ --pre",
    "desa": "ng serve --serve-path /expropia-frontend/ --desa",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "eslint . --fix",
    "e2e": "ng e2e",
    "build:prod": "ng build --base-href /expropia-frontend/ --configuration=prod ",
    "build:tar_local": "npm install && ng build --base-href /expropia-frontend/",
    "build:tar_desa": "npm install && ng build --base-href /expropia-frontend/ --configuration=desa",
    "build:tar_pre": "npm install && ng build --base-href /expropia-frontend/ --configuration=pre",
    "build:tar_prod": "npm install && ng build --base-href /expropia-frontend/ --configuration=prod ",
    "build:tar": "npm install && ng build --base-href /expropia-frontend/ --configuration=prod ",
    "sonar": "node sonar-project.js",
    "coverage": "ng test --codeCoverage=true --browsers=Chrome --watch=false && node sonar-project.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.9",
    "@angular/cdk": "^15.2.9",
    "@angular/common": "^15.2.9",
    "@angular/compiler": "^15.2.9",
    "@angular/core": "^15.2.9",
    "@angular/forms": "^15.2.9",
    "@angular/material": "^15.2.9",
    "@angular/material-moment-adapter": "15.2.9",
    "@angular/platform-browser": "^15.2.9",
    "@angular/platform-browser-dynamic": "^15.2.9",
    "@angular/router": "^15.2.9",
    "@auth0/angular-jwt": "^5.1.2",
    "@ng-matero/extensions": "^15.4.0",
    "@ng-matero/extensions-moment-adapter": "^15.0.0",
    "@ngneat/transloco": "^4.2.2",
    "angular-in-memory-web-api": "0.15.0",
    "arq-sdk": "file:arq-sdk",
    "bootstrap": "^4.3.1",
    "core-js": "3.26.1",
    "date-fns": "^2.29.3",
    "hammerjs": "2.0.8",
    "material-icons": "^1.13.12",
    "moment": "^2.29.4",
    "ngx-cookie-service": "^15.0.0",
    "ngx-toastr": "^16.2.0",
    "primeflex": "^3.3.1",
    "primeicons": "^6.0.1",
    "primeng": "^15.4.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.5.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.3",
    "@angular-eslint/builder": "^15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "~15.2.9",
    "@angular/compiler-cli": "^15.2.9",
    "@angular/language-service": "15.2.9",
    "@types/jasmine": "~4.0.0",
    "@types/jasminewd2": "2.0.10",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "5.48.2",
    "@typescript-eslint/parser": "5.48.2",
    "eslint": "^8.33.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "jasmine-core": "~4.3.0",
    "jasmine-spec-reporter": "^5.0.2",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ngx-build-plus": "^15.0.0",
    "sonarqube-scanner": "^3.4.0",
    "ts-node": "10.9.1",
    "typescript": "~4.9.5"
  }
